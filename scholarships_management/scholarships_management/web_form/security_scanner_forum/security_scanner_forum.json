 "client_script": "function ValidateEmail(inEmail){\r\n    var mailformat = /^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/;\r\n    try {\r\n    if(inEmail.match(mailformat))\r\n        {\r\n            return true;\r\n        }\r\n    else\r\n        {\r\n            return false;\r\n        }\r\n    } catch (err) {\r\n        return false;\r\n    }\r\n}\r\n\r\nfunction ValidateWhatsNumber(inNumber){\r\n    var countryCodeFormat = /^\\+?\\d+$/;\r\n    try{\r\n    if(inNumber.match(countryCodeFormat)[0])\r\n        {\r\n            return true;\r\n        }\r\n    else\r\n        {\r\n            return false;\r\n        }\r\n    } catch (err){\r\n        return false;\r\n    }\r\n}\r\n\r\nfrappe.web_form.validate = () => {\r\n    if (ValidateEmail(frappe.web_form.get_value('email')) && ValidateWhatsNumber(frappe.web_form.get_value('whatsapp_number'))){\r\n        return true ;\r\n    } else {\r\n        return false ;\r\n    }\r\n}\r\n\r\nfrappe.web_form.after_save= () => {\r\n   window.location = frappe.web_form.success_url\r\n}\r\n\r\n",
